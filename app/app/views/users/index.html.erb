<h1>Listing Files</h1>
<%#= link_to "home index", "home/index" %>
<table>
  <tr>
    <th>Filename</th>
    <th>Password</th>
    <th>Days</th>
 
    
   
  </tr>

<% @users.each do |user| %>
  <tr>
    <td width="300px"><%= user.avatar %></td>
    <td width="300px"><%= user.password %></td>
    <td width="300px"><%= user.days %></td>

   
    <td width="300px"><%= link_to 'Show', user %></td>
    
    <td width="300px"><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New File', new_users_path %>
